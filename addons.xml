<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.zaraseries" name="Zara Series" version="0.0.2" provider-name="Zara">
  <requires>
    <import addon="xbmc.python" version="2.14.0"/>
	<import addon="script.module.requests" version="2.4.3"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="main.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <language>en</language>
    <platform>all</platform>
    <summary lang="en">Zara Series</summary>
    <description lang="en">As melhores series você encontra aqui</description>
    <platform>all</platform>
  </extension>
</addon>

